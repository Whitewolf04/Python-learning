   "source": [
    "cake = int(input())\n",
    "calories = input().split()\n",
    "miles = 0\n",
    "\n",
    "calories_list = [int(num) for num in calories]\n",
    "exponent = [things for things in range(0, cake)]\n",
    "final_list = zip(sorted(calories_list)[::-1], exponent)\n",
    "\n",
    "for i, n in final_list:\n",
    "    miles += 2**n * i\n",
    "\n",
    "print(miles)"
   ]
